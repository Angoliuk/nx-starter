{
  "extends": "../../tsconfig.json",
  "include": [],
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "esModuleInterop": false,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["node"],
  },
  "references": [
    {
      "path": "./tsconfig.lib.json"
    }
  ]
}
