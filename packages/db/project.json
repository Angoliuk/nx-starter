{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/db/src",
  "projectType": "library",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "packages/db"
      }
    },
    "push": {
      "command": "prisma db push",
      "options": {
        "cwd": "packages/db"
      }
    },
    "pull": {
      "command": "prisma db pull",
      "options": {
        "cwd": "packages/db"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "packages/db"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "packages/db"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/db/**/*.ts",
          "packages/db/package.json"
        ]
      }
    }
  },
  "tags": []
}
